{
  "name": "infectious-disease-surveillance-cql",
  "version": "1.0.0",
  "description": "傳染病統計監測系統 - CQL 測試與 SMART FHIR 整合",
  "main": "testRunner.js",
  "scripts": {
    "test": "node testRunner.js",
    "test:sample": "node testRunner.js --sample",
    "test:covid": "node testRunner.js --library COVID19",
    "test:flu": "node testRunner.js --library 流感",
    "test:conjunctivitis": "node testRunner.js --library 紅眼症",
    "test:enterovirus": "node testRunner.js --library 腸病毒",
    "test:diarrhea": "node testRunner.js --library 腹瀉"
  },
  "keywords": [
    "CQL",
    "FHIR",
    "SMART",
    "surveillance",
    "infectious-disease",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
